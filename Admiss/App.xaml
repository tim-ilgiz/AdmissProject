<Application x:Class="Admiss.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Admiss"
             xmlns:vm="clr-namespace:Admiss.ViewModel" 
             xmlns:view="clr-namespace:Admiss.View"
             xmlns:theme="clr-namespace:Admiss"
             StartupUri="View/MainWindow.xaml" >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Admiss;component/Theme/App_Theme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
    
    
